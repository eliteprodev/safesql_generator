{
  "version": "1",
  "packages": [
    {
      "path": "go",
      "engine": "postgresql",
      "driver": "pgx/v4",
      "name": "override",
      "schema": "schema.sql",
      "queries": "query.sql",
      "overrides": [
        {
          "go_type": "github.com/kyleconroy/sqlc-testdata/pkg.CustomType",
          "column": "foo.retyped"
        },
        {
          "go_type": "github.com/lib/pq.StringArray",
          "column": "foo.langs"
        }
      ]
    }
  ]
}
